train_net: "models/q_rfcn_tloc_with_ref/train_q_rfcn_tloc_with_ref.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 40000
display: 1000
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "q_rfcn_tloc_with_ref"
clip_gradients: 20
iter_size: 1
# debug_info: true
